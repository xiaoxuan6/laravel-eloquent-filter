{
    "name": "james.xue/laravel-eloquent-filter",
    "description": "An Eloquent way to filter Eloquent Model",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "search",
        "query",
        "filter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "james.xue",
            "email": "1527736751@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "^5.0||^6.0||^7.0||^8.0||^9.0",
        "illuminate/support": "^5.0||^6.0||^7.0||^8.0||^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~v3.4",
        "laravel/framework": "~5.0||~6.0||~7.0||~8.0||~9.0",
        "orchestra/testbench": "^4.0||^5.0||^6.0||^7.0"
    },
    "autoload": {
        "psr-4": {
            "James\\Eloquent\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "James\\Eloquent\\Filter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "James\\Eloquent\\Filter\\FilterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
    },
    "scripts-descriptions": {
        "test": "Run all tests.",
        "fix-style": "Run style checks and fix violations."
    }
}
